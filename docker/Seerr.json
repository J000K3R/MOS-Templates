{
  "name": "seerr",
  "repo": "ghcr.io/seerr-team/seerr:latest",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "sh",
  "privileged": false,
  "extra_parameters": "--restart=unless-stopped",
  "post_parameters": null,
  "web_ui_url": "http://[IP]:[PORT:5055]/",
  "icon": "https://raw.githubusercontent.com/J000K3R/MOS-Templates/main/icons/jellyseerr.png",
  "category": [ "media" ],
  "project": "https://github.com/seerr-team/seerr",
  "support": "https://github.com/seerr-team/seerr/issues",
  "description": "Seerr is a free and open source software application for managing requests for your media library. It integrates with the media server of your choice: Jellyfin, Plex, and Emby. In addition, it integrates with your existing services, such as Sonarr, Radarr.",
  "donate": "https://opencollective.com/seerr",
  "paths": [
    {
      "name": "Appdata",
      "mode": "rw",
      "host": "/mnt/cache/appdata/seerr",
      "container": "/app/config/",
      "description": "Configuration and database files for Jellyseerr."
    }
  ],
  "ports": [
    {
      "name": "Network",
      "protocol": "tcp",
      "host": "5055",
      "container": "5055",
      "description": "Web interface port for Jellyseerr."
    }
  ],
  "variables": [
    {
      "name": "Log",
      "key": "LOG_LEVEL",
      "value": "info",
      "mask": false,
      "description": "Logging verbosity level (debug, info, warn, error)."
    },
    {
      "name": "Emby Support",
      "key": "SEERR_TYPE",
      "value": "type",
      "mask": false,
      "description": "Media server type (jellyfin, plex, emby)."
    },
    {
      "name": "PUID",
      "key": "PUID",
      "value": "1000",
      "mask": false,
      "description": "User ID for file permissions."
    },
    {
      "name": "PGID",
      "key": "PGID",
      "value": "1000",
      "mask": false,
      "description": "Group ID for file permissions."
    },
    {
      "name": "UMASK",
      "key": "UMASK",
      "value": "022",
      "mask": false,
      "description": "File creation mask for new files."
    }
  ],
  "devices": []
}
