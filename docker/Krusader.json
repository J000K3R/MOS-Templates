{
  "name": "Krusader",
  "repo": "ich777/krusader",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "sh",
  "privileged": false,
  "extra_parameters": "--restart=unless-stopped",
  "post_parameters": null,
  "web_ui_url": null,
  "icon": "https://raw.githubusercontent.com/J000K3R/MOS-Templates/main/icons/krusader.png",
  "category": [ "productivity" ],
  "project": "https://krusader.org/",
  "support": "https://invent.kde.org/utilities/krusader/-/issues",
  "description": "Krusader is an advanced twin panel (commander style) file manager for KDE Plasma and other desktops in the *nix world, similar to Midnight or Total Commander. It provides all the file management features you could possibly want.",
  "paths": [
    {
      "name": "Appdata",
      "mode": "rw",
      "host": "/mnt/cache/appdata/krusader",
      "container": "/krusader"
    },
    {
      "name": "Filesystem MOS",
      "mode": "rw",
      "host": "/mnt/cache",
      "container": "/mnt/user"
    }
  ],
  "ports": [
    {
      "name": "WebUI Port",
      "protocol": "tcp",
      "host": "8080",
      "container": "8080"
    }
  ],
  "variables": [
    {
      "name": "USER_LOCALES",
      "key": "USER_LOCALES",
      "value": "en_US.UTF-8 UTF8",
      "mask": false
    },
    {
      "name": "CUSTOM_RES_W",
      "key": "CUSTOM_RES_W",
      "value": "1280",
      "mask": false
    },
    {
      "name": "CUSTOM_RES_H",
      "key": "CUSTOM_RES_H",
      "value": "1024",
      "mask": false
    },
    {
      "name": "UMASK",
      "key": "UMASK",
      "value": "000",
      "mask": false
    },
    {
      "name": "UID",
      "key": "UID",
      "value": "99",
      "mask": false
    },
    {
      "name": "GID",
      "key": "GID",
      "value": "100",
      "mask": false
    }
  ],
  "devices": []
}
