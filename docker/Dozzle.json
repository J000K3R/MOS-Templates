{
  "name": "dozzle",
  "repo": "amir20/dozzle:latest",
  "network": "bridge",
  "custom_ip": null,
  "default_shell": "sh",
  "privileged": false,
  "extra_parameters": "--restart=unless-stopped",
  "post_parameters": null,
  "web_ui_url": "http://[IP]:[PORT:8080]",
  "icon": "https://raw.githubusercontent.com/J000K3R/MOS-Templates/main/icons/dozzle.png",
  "category": [ "productivity" ],
  "project": "https://github.com/amir20/dozzle",
  "support": "https://github.com/amir20/dozzle/issues",
  "description": "Dozzle is a small lightweight application with a web based interface to monitor Docker logs. It doesnâ€™t store any log files. It is for live monitoring of your container logs only.",
  "donate": "https://buymeacoffee.com/amirraminfar",
  "paths": [
    {
      "name": "Docker",
      "mode": "rw",
      "host": "/var/run/docker.sock",
      "container": "/var/run/docker.sock"
    }
  ],
  "ports": [
    {
      "name": "WebUI Port",
      "protocol": "tcp",
      "host": "8080",
      "container": "8080"
    }
  ],
  "variables": [],
  "devices": []
}
